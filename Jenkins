pipeline{
    agent{
        label 'maven'
    }
    stages{
        stage ('build'){
            when{
                branch 'master'
            }
            tools{
                maven 'MAVEN_HOME'
                jdk 'JAVA_HOME'
            }
            steps{
                timestamps{
                 sh 'mvn --version'
                 sh 'mvn install' 
                }
               
            }
        }
    }
}
