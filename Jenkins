pipeline{
    agent{
        label 'maven'
    }
    stages{
        stage ('build'){
            tools{
                maven 'MAVEN_HOME'
                jdk 'JAVA_HOME'
            }
            steps{
                timestamps{
                 sh 'mvn --version'
                 sh 'java -version' 
                }
               
            }
        }
    }
}

